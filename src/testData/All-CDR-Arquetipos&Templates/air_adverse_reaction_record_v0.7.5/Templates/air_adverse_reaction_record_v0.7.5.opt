                <term_definitions code="at0000">
                    <items id="text">Adverse reaction additional details</items>
                    <items id="description">This cluster should include supplementary clinical information about a drug-related adverse reaction that is required for pharmacovigilance notification.</items>
                </term_definitions>
                <term_definitions code="at0001">
                    <items id="text">Attention required</items>
                    <items id="description">Indicate whether the observed adverse drug reaction required any form of medical attention and specify the type of attention received.</items>
                </term_definitions>
                <term_definitions code="at0002">
                    <items id="text">Has the adverse drug reaction required medical attention?</items>
                    <items id="description">Indicate whether the observed adverse drug reaction required any form of medical attention.</items>
                </term_definitions>
                <term_definitions code="at0003">
                    <items id="text">Attention required</items>
                    <items id="description">Indicate the type of medical attention received.</items>
                </term_definitions>
                <term_definitions code="at0004">
                    <items id="text">Yes</items>
                    <items id="description"></items>
                </term_definitions>
                <term_definitions code="at0005">
                    <items id="text">No</items>
                    <items id="description"></items>
                </term_definitions>
                <term_definitions code="at0006">
                    <items id="text">Unknownn</items>
                    <items id="description"></items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="text">Treatment required</items>
                    <items id="description">Indicate whether the observed adverse drug reaction required any form of treatmentand specify the type of treatment received.</items>
                </term_definitions>
                <term_definitions code="at0008">
                    <items id="text">Has the adverse drug reaction required any treatment?</items>
                    <items id="description">Indicate whether the observed adverse drug reaction required any form of treatment.</items>
                </term_definitions>
                <term_definitions code="at0009">
                    <items id="text">Treatment required</items>
                    <items id="description">Indicate the type of treatment received.</items>
                </term_definitions>
                <term_definitions code="at0010">
                    <items id="text">Medical treatment details</items>
                    <items id="description">This cluster should include complementary information about the pharmacological treatment associated with the adverse reaction and required for pharmacovigilance reporting.</items>
                </term_definitions>
                <term_definitions code="at0011">
                    <items id="text">Prescription/vaccination indication</items>
                    <items id="description">Provide detailed information regarding the specific reason for prescribing the medication or administering the vaccination.</items>
                </term_definitions>
                <term_definitions code="at0012">
                    <items id="text">Is the causative agent a vaccine?</items>
                    <items id="description">Indicate whether the adverse reaction was caused by a vaccine. Specify if the agent responsible for the adverse reaction is a vaccine.</items>
                </term_definitions>
                <term_definitions code="at0013">
                    <items id="text">Medication plan</items>
                    <items id="description">Provide a detailed description of the patient's medication plan in plain text format.</items>
                </term_definitions>
                <term_definitions code="at0014">
                    <items id="text">Patient death</items>
                    <items id="description">Indicate if the patient has deceased and provide relevant details regarding the death.</items>
                </term_definitions>
                <term_definitions code="at0015">
                    <items id="text">Date/time of death</items>
                    <items id="description">Provide the precise date and, if available, the exact time of the patient’s death.</items>
                </term_definitions>
                <term_definitions code="at0016">
                    <items id="text">Autopsy</items>
                    <items id="description">Indicate if an autopsy was performed.</items>
                </term_definitions>
                <term_definitions code="at0017">
                    <items id="text">Certainty - Additional details</items>
                    <items id="description">Provide additional details to support the certainty or causality assessment of the adverse reaction.</items>
                </term_definitions>
                <term_definitions code="at0019">
                    <items id="text">Actions taken</items>
                    <items id="description">Provide a detailed account of the actions taken in response to the adverse reaction.</items>
                </term_definitions>
                <term_definitions code="at0020">
                    <items id="text">Observations</items>
                    <items id="description">Provide any relevant observations related to the adverse reaction that can assist in the pharmacovigilance reporting process.</items>
                </term_definitions>
                <term_definitions code="at0021">
                    <items id="text">Additional details</items>
                    <items id="description">Provide any extra information that may be relevant to the pharmacovigilance report.</items>
                </term_definitions>
