                            <items id="text">Lleu</items>
                        </term_definitions>
                        <term_definitions code="local_terms_1::6736007">
                            <items id="text">Moderat</items>
                        </term_definitions>
                        <term_definitions code="local_terms_1::371924009">
                            <items id="text">Moderat greu</items>
