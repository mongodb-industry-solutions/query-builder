                        <term_definitions code="SNOMED CT::311931000135100">
                            <items id="text">VPH positiu no 16/18</items>
                        </term_definitions>
                        <term_definitions code="SNOMED CT::125154007">
                            <items id="text">Inadequat</items>
                        </term_definitions>
