definition
	CLUSTER[at0000] matches {    -- Gestió de la pacient
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Deslliurament
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0009,    -- dirigit
							at0010,    -- espontani
							at0011,    -- manual
							at0012]    -- quirúrgic
						}
					}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- Posició durant l'expulsiu
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0013,    -- 4 grapes
							at0014,    -- gatzoneta
							at0015,    -- lateral
							at0016,    -- asseguda
							at0017,    -- vertical
							at0018,    -- decubit
							at0019]    -- altres
						}
					}
				}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Indicació de la tocoquirúrgica
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0020,    -- Abreujament expulsiu
							at0021,    -- Distòcia rotació
							at0022,    -- SPBF-Sospita pèrdua benestar fetal
							at0023,    -- Cesària electiva
							at0024,    -- Inducció fallida
							at0025,    -- Part estacionat
							at0026,    -- DPF-desproporció fetus-pelvis
							at0027,    -- Materna
							at0028,    -- No instrument
							at0029,    -- Cesària iterativa
							at0030]    -- Altres
						}
					}
				}
			}
			ELEMENT[at0004] occurrences matches {0..1} matches {    -- Motiu de cesària electiva
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0031,    -- cesarea iterativa
							at0032,    -- presentació no cefàlica
							at0033,    -- CIR - retard creixement intrauterí
							at0034,    -- sospita de macrosoma
							at0035,    -- patologia materna
							at0036,    -- Placenta previa
							at0037,    -- gestació múltiple
							at0038,    -- renuncia part vaginal
							at0039]    -- altres
						}
					}
				}
			}
			ELEMENT[at0005] occurrences matches {0..1} matches {    -- Motiu de cesària intrapart
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0040,    -- DPF - desproporció Fetos - pelvis
							at0041,    -- SPBF - sospita de pèrdua benestar fetal
							at0042,    -- fracàs d' inducció
							at0043,    -- instrumentació fallida
							at0044,    -- distocia de 2 periode del part
							at0045]    -- altres
						}
					}
				}
			}
			ELEMENT[at0006] occurrences matches {0..*} matches {    -- Revisió canal de part
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0046,    -- Íntegre
							at0047,    -- episiotomia
							at0048,    -- estrip perineal I
							at0049,    -- estrip perineal II
							at0050,    -- estrip perineal IIIA
							at0051,    -- estrip perineal IIIB
							at0052,    -- estrip perineal IV
							at0053,    -- estrip vaginal
							at0054,    -- estrip cervical
							at0055,    -- estrip uretral
							at0056,    -- estrip vesical
							at0057,    -- ruptura uterina
							at0058,    -- deshicència cicatriu
							at0059]    -- Hematomes vaginals
						}
					}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Inici del part
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0060,    -- espontani
							at0061,    -- induït
							at0062,    -- cessàrea electiva
							at0063]    -- cessàrea urgent
						}
					}
				}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {    -- Part en l'aigua
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0064,    -- Si
							at0065,    -- No
							at0066]    -- Desconegut
						}
					}
				}
			}
		}
	}

