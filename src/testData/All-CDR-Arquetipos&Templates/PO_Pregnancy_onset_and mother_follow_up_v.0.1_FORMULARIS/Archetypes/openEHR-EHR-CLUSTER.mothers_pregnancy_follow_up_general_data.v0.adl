			}
			ELEMENT[at0081] occurrences matches {0..1} matches {    -- Proteinuria
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0082,    -- Negative
							at0083,    -- Positive
							at0084,    -- +
							at0085,    -- ++
							at0086]    -- +++
						}
					}
				}
			}
			ELEMENT[at0087] occurrences matches {0..1} matches {    -- Edema
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0088,    -- Absence of edema
							at0089,    -- Edema in the feet, malleolar and pretibial
							at0090,    -- Edema in the lower extremities
							at0091,    -- Edema of the face, hands, abdominal wall and sacrum
							at0092]    -- Anasarca with ascites
						}
					}
				}
			}
			ELEMENT[at0093] occurrences matches {0..1} matches {    -- Uterine height
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"cm">
							>
