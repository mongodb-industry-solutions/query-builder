definition
	CLUSTER[at0000] matches {    -- General data for monitoring the mother's pregnancy
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Gestation week
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::128]>
						list = <
							["1"] = <
								units = <"wk">
							>
						>
					>
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- Pregnancy risk classification
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0003,    -- Low risk
							at0004,    -- Medium risk
							at0005,    -- High risk
							at0006]    -- Very high risk
						}
					}
				}
			}
			ELEMENT[at0007] occurrences matches {0..*} matches {    -- Pregnancy risk
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0008,    -- EBR: Pelvic anomalies
							at0009,    -- EBR: Short height (less than 145 cm)
							at0010,    -- EBR: Inadequate Body Mass Index (<18.5 or >30)
							at0011,    -- EBR: Unwanted pregnancy
							at0012,    -- EBR: Inadequate vaccination status
							at0013,    -- EBR: maternal cardiovascular risk WHO I
							at0014,    -- ERM: Very early age (less than 15 years)
							at0015,    -- ERM: Elderly woman (≥ 40 years)
							at0016,    -- ERM: Great multiparity (woman who has had 4 or more births)
							at0017,    -- ERM: Intergenic period < 12 months
							at0018,    -- ERM: Antecedents of intrauterine growth retardation and low birth weight infants
							at0019,    -- ERM: Dystosic parts
							at0020,    -- ERM: Previous uterine surgery
							at0021,    -- ERM: Previous caesarean section
							at0022,    -- ERM: Previous sterility
							at0023,    -- ERM: Antecedents of placental pathology
							at0024,    -- ERM: Antecedents of obstetric pathology in previous pregnancies
							at0025,    -- ERM: Insufficient or inadequate control of the pregnant woman
							at0026,    -- ERM: Unfavorable socio-economic conditions
							at0027,    -- ERM: Victim of gender violence
							at0028,    -- ERM: Personal history of mental illness
							at0029,    -- ERM: Current controlled and stable mental pathology
							at0030,    -- ERM: Metrorrhagia 1st. quarter
							at0031,    -- ERM: Gestational diabetes
							at0032,    -- ERM: Low risk of urinary tract infection or bacterial, Zika or STI
							at0033,    -- ERM: Regular smoker
							at0034,    -- ERM: Rh -
							at0035,    -- ERM: Cardiovascular risk WHO II
							at0036,    -- ERM: Family history of hereditary diseases
							at0037,    -- ERA: Morbid obesity (BMI >40)
							at0038,    -- ERA: History of abortions (2 or more)
							at0039,    -- ERA: Previous uterine cervical incompetence
							at0040,    -- ERA: Previous chromosomal abnormality
							at0041,    -- ERA: Previous ectopic pregnancy
							at0042,    -- ERA: Previous gestational trophoblastic disease
							at0043,    -- ERA: Previous preterm birth
							at0044,    -- ERA: Endocrinopathy
							at0045,    -- ERA: Maternal cardiovascular risk WHO III
							at0046,    -- ERA: Hypertension prior to pregnancy
							at0047,    -- WAS: Twins
							at0048,    -- ERA: Gestational diabetes corrected with diet and insulin
							at0049,    -- ERA: Mild preeclampsia (>20 sep. gestation) >140/90 mm Hg and <160/110 mm Hg
							at0050,    -- ERA: Maternal infection diagnosed during pregnancy
							at0051,    -- ERA: Isoimmunization - Indirect Coombs test (+) in mother Rh (-)
							at0052,    -- ERA: Severe current mental illness
							at0053,    -- ERA: Severe anemia (Hb <9 g/dl and Hematocrit <25 %)
							at0054,    -- ERA: Suspected fetal malformation
							at0055,    -- ERMA: Severe associated pathology
							at0056,    -- ERMA: Previous perinatal death
							at0057,    -- ERMA: Diabetes Mellitus type 1 or 2
							at0058,    -- ERMA: Maternal cardiovascular risk WHO IV
							at0059,    -- ERMA: Alcohol and/or other drug dependence syndrome
							at0060,    -- ERMA: Diagnosed uterine malformations
							at0061,    -- ERMA: Multiple gestation (>3 fetuses in the current pregnancy)
							at0062,    -- ERMA: Intrauterine Growth Retardation (IRG)
							at0063,    -- ERMA: Confirmed fetal malformation
							at0064,    -- ERMA: Anomalous situation of the placenta
							at0065,    -- ERMA: Severe preeclampsia
							at0066,    -- ERMA: Premature birth threaten
							at0067]    -- ERMA: Premature rupture of membranes in the preterm
						}
					}
				}
			}
			ELEMENT[at0071] occurrences matches {0..1} matches {    -- Early preeclampsia risk
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0072,    -- Low risk
							at0073,    -- Risk
							at0074,    -- Not assessable
							at0075]    -- Not applicable
						}
					}
				}
			}
			ELEMENT[at0129] occurrences matches {0..1} matches {    -- DV_TEXT
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0130] occurrences matches {0..1} matches {    -- DV_TEXT
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0076] occurrences matches {0..1} matches {    -- Risk fraction of preeclampsia
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::507]>
						list = <
							["1"] = <
								units = <"/10*3">
							>
						>
					>
				}
			}
			ELEMENT[at0077] occurrences matches {0..1} matches {    -- Weight
				value matches {
