					>
				}
			}
			ELEMENT[at0094] occurrences matches {0..1} matches {    -- Symptoms - general
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0095,    -- Asymptomatic
							at0096,    -- Various
							at0097]    -- Absence of fetal movements
						}
					}
				}
			}
			ELEMENT[at0098] occurrences matches {0..1} matches {    -- Symptoms - vulvovaginal discomfort
