						defining_code matches {
							[local::
							at0099,    -- Vulvovaginal itching
							at0100,    -- Leucorrhea
							at0101]    -- Vulvar ulcers
						}
					}
				}
			}
			ELEMENT[at0102] occurrences matches {0..1} matches {    -- Symptoms - digestive disorders
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0103,    -- Nausea
							at0104,    -- Vomiting
							at0105,    -- Heartburn
							at0106,    -- Diarrhea
							at0107,    -- Constipation
